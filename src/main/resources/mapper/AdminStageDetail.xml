<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.team.gyemoim.mapper.admin.AdminStageDetailMapper">
    <!--(현지)리스트 띄우기  -->
    <select id="getStageList" resultType="com.team.gyemoim.dto.admin.AdminStageDetailDTO">
        SELECT *
        FROM pf
        ORDER BY  startDate
    </select>


    <select id="getStageListDetail1" resultType="com.team.gyemoim.dto.admin.AdminStageDetailDTO">
        SELECT *
        FROM pf
        WHERE pfID=#{pfID}
    </select>

    <select id="getStageListDetail2" resultType="com.team.gyemoim.dto.admin.AdminStageDetailDTO">
        SELECT *
        FROM roll
        WHERE pfID=#{pfID}
    </select>

</mapper>